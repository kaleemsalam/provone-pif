<!-- CalculationProcess BPEL Process [Generated by the Eclipse BPEL Designer] -->
<!-- Date: Fri Aug 12 09:12:24 CEST 2016 -->
<bpel:process name="CalculationProcess"
              targetNamespace="http://demo.kit"
              suppressJoinFailure="yes"
              xmlns:tns="http://demo.kit"
              xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
              xmlns:ns="http://kit.demo/">

    <!-- Import the client WSDL -->
    <bpel:import namespace="http://kit.demo/" location="simpleCalculator.wsdl"
                 importType="http://schemas.xmlsoap.org/wsdl/"></bpel:import>
    <bpel:import location="CalculationProcessArtifacts.wsdl" namespace="http://demo.kit"
                 importType="http://schemas.xmlsoap.org/wsdl/"/>

    <!-- ================================================================= -->
    <!-- PARTNERLINKS                                                      -->
    <!-- List of services participating in this BPEL process               -->
    <!-- ================================================================= -->
    <bpel:partnerLinks>
        <!-- The 'client' role represents the requester of this service. -->
        <bpel:partnerLink name="client"
                          partnerLinkType="tns:CalculationProcess"
                          myRole="CalculationProcessProvider"
        />
        <bpel:partnerLink name="simpleCalculatorPL" partnerLinkType="tns:simpleCalculatorPLT"
                          partnerRole="simpleCalculatorPLT"></bpel:partnerLink>
    </bpel:partnerLinks>

    <!-- ================================================================= -->
    <!-- VARIABLES                                                         -->
    <!-- List of messages and XML documents used within this BPEL process  -->
    <!-- ================================================================= -->
    <bpel:variables>
        <!-- Reference to the message passed as input during initiation -->
        <bpel:variable name="input"
                       messageType="tns:CalculationProcessRequestMessage"/>

        <!-- 
          Reference to the message that will be returned to the requester
          -->
        <bpel:variable name="output" messageType="tns:CalculationProcessResponseMessage"/>

        <bpel:variable name="simpleCalculatorPLResponse" messageType="ns:AddResponse"></bpel:variable>
        <bpel:variable name="simpleCalculatorPLRequest" messageType="ns:Add"></bpel:variable>
        <bpel:variable name="simpleCalculatorPLResponse1" messageType="ns:squareResponse"></bpel:variable>
        <bpel:variable name="simpleCalculatorPLRequest1" messageType="ns:square"></bpel:variable>
    </bpel:variables>

    <!-- ================================================================= -->
    <!-- ORCHESTRATION LOGIC                                               -->
    <!-- Set of activities coordinating the flow of messages across the    -->
    <!-- services integrated within this business process                  -->
    <!-- ================================================================= -->
    <bpel:sequence name="main">

        <!-- Receive input from requester. 
             Note: This maps to operation defined in CalculationProcess.wsdl 
             -->
        <bpel:receive name="receiveInput" partnerLink="client"
                      portType="tns:CalculationProcess"
                      operation="process" variable="input"
                      createInstance="yes"/>

        <!-- Generate reply to synchronous request -->
        <bpel:assign validate="no" name="AssignInputToService">
            <bpel:copy>
                <bpel:from>
                    <bpel:literal>
                        <tns:Add xmlns:tns="http://kit.demo/">
                            <firstNum>0.0</firstNum>
                            <secondNum>0.0</secondNum>
                        </tns:Add>
                    </bpel:literal>
                </bpel:from>
                <bpel:to variable="simpleCalculatorPLRequest" part="parameters"></bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from part="payload" variable="input">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">
                        <![CDATA[tns:num1]]></bpel:query>
                </bpel:from>
                <bpel:to part="parameters" variable="simpleCalculatorPLRequest">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">
                        <![CDATA[firstNum]]></bpel:query>
                </bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from part="payload" variable="input">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">
                        <![CDATA[tns:num2]]></bpel:query>
                </bpel:from>
                <bpel:to part="parameters" variable="simpleCalculatorPLRequest">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">
                        <![CDATA[secondNum]]></bpel:query>
                </bpel:to>
            </bpel:copy>
        </bpel:assign>
        <bpel:invoke name="InvokeAddOperation" partnerLink="simpleCalculatorPL" operation="Add"
                     portType="ns:SimpleCalculator" inputVariable="simpleCalculatorPLRequest"
                     outputVariable="simpleCalculatorPLResponse"></bpel:invoke>
        <bpel:assign validate="no" name="AssignServiceOutputToSquareOperation">
            <bpel:copy>
                <bpel:from>
                    <bpel:literal>
                        <tns:square xmlns:tns="http://kit.demo/">
                            <number>0.0</number>
                        </tns:square>
                    </bpel:literal>
                </bpel:from>
                <bpel:to variable="simpleCalculatorPLRequest1" part="parameters"></bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from>
                    <bpel:literal>
                        <tns:CalculationProcessResponse xmlns:tns="http://demo.kit"
                        >
                            <tns:result>0.0</tns:result>
                        </tns:CalculationProcessResponse>
                    </bpel:literal>
                </bpel:from>
                <bpel:to variable="output" part="payload"></bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from part="parameters" variable="simpleCalculatorPLResponse">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">
                        <![CDATA[return]]></bpel:query>
                </bpel:from>
                <bpel:to part="parameters" variable="simpleCalculatorPLRequest1">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">
                        <![CDATA[number]]>
                    </bpel:query>
                </bpel:to>
            </bpel:copy>
        </bpel:assign>
        <bpel:invoke name="InvokeSqureOperation" partnerLink="simpleCalculatorPL" operation="square"
                     portType="ns:SimpleCalculator" inputVariable="simpleCalculatorPLRequest1"
                     outputVariable="simpleCalculatorPLResponse1"></bpel:invoke>
        <bpel:assign validate="no" name="AssignToOutput">
            <bpel:copy>
                <bpel:from>
                    <bpel:literal>
                        <tns:CalculationProcessResponse xmlns:tns="http://demo.kit"
                        >
                            <tns:result>0.0</tns:result>
                        </tns:CalculationProcessResponse>
                    </bpel:literal>
                </bpel:from>
                <bpel:to variable="output" part="payload"></bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from part="parameters" variable="simpleCalculatorPLResponse1">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">
                        <![CDATA[return]]></bpel:query>
                </bpel:from>
                <bpel:to part="payload" variable="output">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">
                        <![CDATA[tns:result]]></bpel:query>
                </bpel:to>
            </bpel:copy>
        </bpel:assign>
        <bpel:reply name="replyOutput"
                    partnerLink="client"
                    portType="tns:CalculationProcess"
                    operation="process"
                    variable="output"
        />

    </bpel:sequence>


</bpel:process>

