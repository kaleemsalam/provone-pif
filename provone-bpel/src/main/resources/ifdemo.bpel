<bpel:process name="ifdemo"
         targetNamespace="http://bpeldemo.kit.edu"
         suppressJoinFailure="yes"
         xmlns:tns="http://bpeldemo.kit.edu"
         xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         >

    <!-- Import the client WSDL -->
	<bpel:import location="ifdemoArtifacts.wsdl" namespace="http://bpeldemo.kit.edu" 
	        importType="http://schemas.xmlsoap.org/wsdl/" />
         
    <!-- ================================================================= -->         
    <!-- PARTNERLINKS                                                      -->
    <!-- List of services participating in this BPEL process               -->
    <!-- ================================================================= -->         
    <bpel:partnerLinks>
        <!-- The 'client' role represents the requester of this service. -->
        <bpel:partnerLink name="client"
                     partnerLinkType="tns:ifdemo"
                     myRole="ifdemoProvider"
                     />
    </bpel:partnerLinks>
  
    <!-- ================================================================= -->         
    <!-- VARIABLES                                                         -->
    <!-- List of messages and XML documents used within this BPEL process  -->
    <!-- ================================================================= -->         
    <bpel:variables>
        <!-- Reference to the message passed as input during initiation -->
        <bpel:variable name="input"
                  messageType="tns:ifdemoRequestMessage"/>
                  
        <!-- 
          Reference to the message that will be returned to the requester
          -->
        <bpel:variable name="output"
                  messageType="tns:ifdemoResponseMessage"/>
    </bpel:variables>

    <!-- ================================================================= -->         
    <!-- ORCHESTRATION LOGIC                                               -->
    <!-- Set of activities coordinating the flow of messages across the    -->
    <!-- services integrated within this business process                  -->
    <!-- ================================================================= -->         
    <bpel:sequence name="main">
        
        <!-- Receive input from requester. 
             Note: This maps to operation defined in ifdemo.wsdl 
             -->
        <bpel:receive name="receiveInput" partnerLink="client"
                 portType="tns:ifdemo"
                 operation="process" variable="input"
                 createInstance="yes"/>
        
        <!-- Generate reply to synchronous request -->
        
        
        
        <bpel:if name="If">
            <bpel:condition expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">
            <![CDATA[$input.payload/tns:input2 > 0]]></bpel:condition>
            <bpel:assign validate="no" name="Assign">
                
                <bpel:copy>
                    <bpel:from><bpel:literal><tns:ifdemoResponse xmlns:tns="http://bpeldemo.kit.edu" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:result>tns:result</tns:result>
</tns:ifdemoResponse>
</bpel:literal></bpel:from>
                    <bpel:to variable="output" part="payload"></bpel:to>
                </bpel:copy>
                <bpel:copy>
                    <bpel:from part="payload" variable="input">
                        <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:input1]]></bpel:query>
                    </bpel:from>
                    <bpel:to part="payload" variable="output">
                        <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:result]]></bpel:query>
                    </bpel:to>
                </bpel:copy>
            </bpel:assign>
            <bpel:else>
                <bpel:assign validate="no" name="AssignError">
                    
                    <bpel:copy>
                        <bpel:from><bpel:literal><tns:ifdemoResponse xmlns:tns="http://bpeldemo.kit.edu" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:result>tns:result</tns:result>
</tns:ifdemoResponse>
</bpel:literal></bpel:from>
                        <bpel:to variable="output" part="payload"></bpel:to>
                    </bpel:copy>
                    <bpel:copy>
                        <bpel:from>
                            <bpel:literal>Error
</bpel:literal>
                        </bpel:from>
                        <bpel:to part="payload" variable="output">
                            <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">
                                <![CDATA[tns:result]]>
                            </bpel:query>
                        </bpel:to>
                    </bpel:copy>
                </bpel:assign>
            </bpel:else>
        </bpel:if>
        
        <bpel:reply name="replyOutput" 
               partnerLink="client"
               portType="tns:ifdemo"
               operation="process" 
               variable="output"
               />
    </bpel:sequence>
</bpel:process>

